}

/*QToolButton部分-end------------------------------------------------------------------------------------------------------------------*/



/*QScrollBar部分-start------------------------------------------------------------------------------------------------------------------*/
QScrollArea
{
	background-color: rgba(255,255,255,255);		
}

QScrollBar
{
	background-color: rgba(0,0,0,0);	
}

QScrollBar:vertical 
{
	width: 8px;
	padding-top:0px;
	padding-bottom:0px;
	padding-right:1px;
	padding-left:1px; 	
	border: 0px;
	background: none;
	margin: 0px;
}

QScrollBar::add-line:vertical
{
	background-color: rgba(0,0,0,0);
}

QScrollBar::sub-line:vertical
{
	background-color: rgba(0,0,0,0);
}

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical 
{
    background: none;
}
/*QScrollBar部分-end------------------------------------------------------------------------------------------------------------------*/



