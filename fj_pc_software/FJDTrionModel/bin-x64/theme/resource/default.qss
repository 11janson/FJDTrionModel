/*QWidget部分-start------------------------------------------------------------------------------------------------------------------*/

QWidget
{
    background-color:#1c1c1c;
	border-color: #1c1c1c;
	color:#ffffff;
	font-family:"Microsoft YaHei UI";
}

SARibbonBar QMenuBar
{
    background-color:#1c1c1c;
	border-color: #1c1c1c;
	color:#ffffff;
	font-family:"Microsoft YaHei UI";
}


/*QWidget部分-end------------------------------------------------------------------------------------------------------------------*/



/*QTabWidget部分-start------------------------------------------------------------------------------------------------------------------*/
QTabWidget
{
    background-color:#ff0000;
	border-color: #2B2B2B;
	color:#ffffff;
}

QTabWidget::pane
{
    background-color:#ff0000;
	border-color: #2B2B2B;
	color:#ffffff;
}

QTabWidget>QTabBar::tab
{
	color: #ffffff;
	background-color: #2B2B2B
	border: #2B2B2B
	border-left: #2B2B2B
	border-bottom: #2B2B2B
}

QTabWidget>QTabBar::tab:selected
{
	color: #ffffff;
	background-color: #484848
	border: #2B2B2B
	border-left: #2B2B2B
	border-bottom: #2B2B2B
}



/*QTabWidget部分-end------------------------------------------------------------------------------------------------------------------*/


/*QPushButton部分-start------------------------------------------------------------------------------------------------------------------*/

QPushButton:hover
{
	background-color:#555555;
}

QPushButton:checked
{
	color: #ffffff;
	border-bottom:2px solid #B0C4DE;
	background-color:#656565;
}

QPushButton
{
	color: #ffffff;
	background-color:#484848;
	border-bottom:2px solid #B0C4DE;
}

QPushButton:pressed
{
	background-color:#656565;
}

/*QPushButton部分-end------------------------------------------------------------------------------------------------------------------*/







/*QTreeView部分-start------------------------------------------------------------------------------------------------------------------*/
QTreeView
  {
	font: 13px;
	show-decoration-selected:1;
	alternate-background-color: #484848;
	background-color: #484848;
	border: 1px solid #B0C4DE;
	color:#ffffff;
  }

 /*所有树：条目列高为27*/
QTreeView::item
 {
	height: 27px;
 }
 
  /* dockWidget中的树：鼠标悬浮于条目上时，该条目背景颜色变为灰色 */
QTreeView::item:hover,QTreeView::branch:hover
 {
    background: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #ffe7bf, stop:0.2 #ffd376,stop:0.7 #ffe688);
	color: #ffffff;
 }

  /*所有树：条目被选中时背景颜色变为浅蓝,文字颜色*/
QTreeView::item:selected,QTreeView::branch:selected  
 {
     background: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #ffd178, stop:0.3 #ffc45d,stop:0.7 #ffd691);
	 color: #ffffff;
 }
 
QTreeView::branch
  {
	background-color:transparent;
  }
 
QTreeView>QHeaderView::down-arrow 
{
    image: url(theme/qssimage/mainwidget_downarrow_normal.png);
}

QTreeView>QHeaderView::up-arrow 
{
    image: url(theme/qssimage/mainwidget_uparrow_normal.png);
}
QTreeView>QHeaderView::down-arrow:hover 
{
    image: url(theme/qssimage/mainwidget_downarrow_hover.png);
}

QTreeView>QHeaderView::up-arrow:hover
{
    image: url(theme/qssimage/mainwidget_uparrow_hover.png);
}
 
QTreeView>QHeaderView::section 
{
    font: 14px;
    background: qlineargradient(spread:pad, x1:0, y1:1, x2:0, y2:0, stop:0 #c4ddff, stop:0.55 #e1eeff);
	padding: 10px;
	padding-left: 14px;
	padding-bottom: 9px;
	border: 1px solid #B0C4DE;
	border-right:0;
	height:12px;
}
QTreeView >QHeaderView::section:first 
{
	width:35px;
}

   /*QTreeWidgetItem未勾选状态，选框图片及背景图片*/
QTreeView::indicator:unchecked
{
	image: url(theme/qssimage/checkbox_unchecked_normal.png);
}
 /*QTreeWidgetItem勾选状态，选框图片及背景图片*/
QTreeView::indicator:checked 
{
	image: url(theme/qssimage/checkbox_checked_normal.png);
}
 /*QTreeWidgetItem未勾选hover状态，选框图片*/
QTreeView::indicator:unchecked:hover
{
	image: url(theme/qssimage/checkbox_unchecked_hover.png);
}
 /*QTreeWidgetItem勾选hover状态，选框图片及背景图片*/
QTreeView::indicator:checked:hover
{
    image: url(theme/qssimage/checkbox_checked_hover.png);
}
 /*QTreeWidgetItem未勾选状态，选框图片*/
QTreeView::indicator:unchecked:disabled
{
	image: url(theme/qssimage/checkbox_unchecked_disabled.png);
}
 /*QTreeWidgetItem勾选状态，选框图片及背景图片*/
QTreeView::indicator:checked:disabled
{
	image: url(theme/qssimage/checkbox_checked_disabled.png);
}

/*QTreeView部分-end------------------------------------------------------------------------------------------------------------------*/





/*QToolButton部分-start------------------------------------------------------------------------------------------------------------------*/
QToolButton
{
	border:0px solid ;
	background-color: #122d3e;
	color: #ade3ff;
	font: 15px;
	min-height: 42px;
	max-height: 42px;
	font-family:"微软雅黑";
}

QToolButton:hover
{
