/*QSlider部分-start------------------------------------------------------------------------------------------------------------------*/
QSlider
{
	background-color:transparent;
}

/*进度条*/
QSlider::groove:horizontal
{
	border-radius:3px;
	height:6px;
}
/*滑动块设置水平*/
QSlider::handle:horizontal
{
	border:0px;
	height: 16px;
	width:16px;
	background:transparent;
	image:url(theme/qssimage/movebutton.png);
}
/*滑过水平*/
QSlider::sub-page:horizontal
{
	border-radius:3px;
	background-color: qlineargradient(spread:pad, x1:1, y1:0.75, x2:1, y2:0, stop:0 #0270c7, stop:1 #008df9);
}
/*为滑过水平*/
QSlider::add-page:horizontal
{
	border-radius:3px;
	background-color: qlineargradient(spread:pad, x1:1, y1:0.75, x2:1, y2:0, stop:0 #96abc4, stop:1 #9ab1cd);
}
/*滑动块设置竖直*/
QSlider::handle:vertical
{
	border:0px;
	height: 16px;
	width:16px;
	background:transparent;
	image:url(theme/qssimage/movebutton.png);
}
/*滑过竖直*/
QSlider::sub-page:vertical
{
	border-radius:3px;
	background-color: qlineargradient(spread:pad, x1:0.478591, y1:0, x2:1, y2:0, stop:0 #0270c7, stop:1 #008df9);
}
/*为滑过竖直*/
QSlider::add-page:vertical
{
	border-radius:3px;
	background-color: qlineargradient(spread:pad, x1:0.478591, y1:0, x2:1, y2:0, stop:0 #96abc4, stop:1 #9ab1cd);
}
/*QSlider部分-end------------------------------------------------------------------------------------------------------------------*/







/*QGroupBox部分-start------------------------------------------------------------------------------------------------------------------*/
QGroupBox
{
	border-radius: 0px;
	font-size: 14px;
	background-color: #484848;
	border-top:38px solid #B0C4DE;
	border-left: 1px solid #B0C4DE;
	border-right: 1px solid #B0C4DE;
	border-bottom: 1px solid #B0C4DE;
	color: #ffffff;
}

QGroupBox::title
{
	subcontrol-origin: margin;
	subcontrol-position: top center;
	padding-top: 5px;
	color: #ffffff;
	background-color: #2B2B2B;
}

QGroupBox::indicator:checked 
{
	image: url(theme/qssimage/checkbox_checked_normal.png);
}

QGroupBox::indicator:unchecked
{
	image: url(theme/qssimage/checkbox_unchecked_normal.png);
}

QGroupBox[AsBorder = true]
{
	border:1px solid #B0C4DE;
}

QGroupBox#groupBox_Weather
{
	border-top:28px solid #B0C4DE;
}

QGroupBox#groupBox_Weather::title
{
	color: #ffffff;
}

QGroupBox#groupBox_Elect
{
	border-top:28px solid #B0C4DE;
}

QGroupBox#groupBox_Elect::title
{
	color: #ffffff;
}


QGroupBox#groupBox_End
{
	border-top:28px solid #B0C4DE;
}

QGroupBox#groupBox_End::title
{
	color: #ffffff;
}

QGroupBox#groupBox_End::indicator
{
	color:#ffffff;
}
/*QGroupBox部分-end------------------------------------------------------------------------------------------------------------------*/







